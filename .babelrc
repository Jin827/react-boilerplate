{
  "presets": [
    "react", [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        },
        "loose": true,
        "modules": false
      }
    ],
    "babel-preset-react"
  ],
  "retainLines": true,
  "plugins": [
    "syntax-dynamic-import",
    "dynamic-import-webpack",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        "react-hot-loader/babel"
      ]
    }
  }
}
