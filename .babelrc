{
  "presets": [
    "react", [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    "babel-preset-react"
  ],
  "retainLines": true,
  "plugins": ["syntax-dynamic-import", "transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "development": {
      "plugins": ["transform-runtime", "react-hot-loader/babel", "transform-flow-strip-types"]
    }
  }
}
