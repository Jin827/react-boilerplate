{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "dev-init": "webpack-dev-server --config=webpack.dev.js",
    "precommit": "lint-staged",
    "flow-test": "flow; test $? -eq 0 -o $? -eq 2",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "NODE_ENV=development webpack --config webpack.dev.js --watch",
    "dev:server": "NODE_ENV=development nodemon --inspect --watch config --watch server server/server.js",
    "server": "nodemon --watch public --exec \"node public/dist/main-bundle.js\"",
    "build:prod": "NODE_ENV=production webpack --config=webpack.prod.js",
    "start": "NODE_ENV=production node server/server.js",
    "heroku-postbuild": "npm run build:prod"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,json,css,scss}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      ".*/dist",
      ".*/node_modules",
      "./package.json",
      "./package-lock.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jin827/react-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jin827/react-boilerplate/issues"
  },
  "homepage": "https://github.com/Jin827/react-boilerplate#readme",
  "dependencies": {
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "brotli-webpack-plugin": "^1.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "cookie-parser": "^1.4.3",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.1",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "markdown-loader": "^3.0.0",
    "markdown-with-front-matter-loader": "^0.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
